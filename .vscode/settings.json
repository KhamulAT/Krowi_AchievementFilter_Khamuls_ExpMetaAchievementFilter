{
  "emeraldwalk.runonsave": {
    "commands": [
    {
      "match": "\\.(lua|toc|xml)$",
      "cmd": "powershell.exe -NoProfile -ExecutionPolicy Bypass -File \"${workspaceFolder}\\bin\\publish2wowdir.ps1\""
    }]
  },
  "Lua.runtime.version": "Lua 5.1",
  "Lua.runtime.builtin": {
    "basic": "disable",
    "debug": "disable",
    "io": "disable",
    "math": "disable",
    "os": "disable",
    "package": "disable",
    "string": "disable",
    "table": "disable",
    "utf8": "disable"
  },
  "Lua.workspace.library": [
    "~\\.vscode\\extensions\\ketho.wow-api-0.21.0\\Annotations\\Core"
  ],
  "Lua.diagnostics.globals": [
    "ACCEPT",
    "CANCEL",
    "Settings",
    "SELECTED_CHAT_FRAME",
    "DEFAULT_CHAT_FRAME",
    "SlashCmdList",
    "hash_SlashCmdList",
    "NORMAL_FONT_COLOR_CODE",
    "FONT_COLOR_CODE_CLOSE"
  ]
}